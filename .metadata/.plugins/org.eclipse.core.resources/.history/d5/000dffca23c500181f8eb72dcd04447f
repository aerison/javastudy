package day06;

public class Book {

	 public Book() {}
	 public Book(String title, int price) {
	  this.title = title;
	  this.price = price;
	 }
	 
	 private String title;
	 private int price;
	 
	 public String getTitle() {
	  return title;
	 }
	 public void setTitle(String title) {
	  this.title = title;
	 }
	 public int getPrice() {
	  return price;
	 }
	 public void setPrice(int price) {
	  this.price = price;
	 }
	 
	 public void print() {
	  System.out.println("title : " + title + " / price : " + price);
	 }
	 
	} 